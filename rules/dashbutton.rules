rule "Dash button pressed chiudi serrande"
    when
        Channel "amazondashbutton:dashbutton:finish:press" triggered
    then
        if (TapparellaLettoPosition.state > 0) {
                TapparellaLettoPosition.sendCommand(0)
        }
        else 
        {
                TapparellaLettoPosition.sendCommand(100)
        }
end